{
  "name": "antd-management-fast-template",
  "version": "1.7.18",
  "private": true,
  "description": "antd-management-fast-template",
  "keywords": [
    "antd-management-fast-template"
  ],
  "homepage": "https://github.com/kityandhero/antd-management-fast-template.git",
  "bugs": {
    "url": "https://github.com/kityandhero/antd-management-fast-framework/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kityandhero/antd-management-fast-template.git"
  },
  "license": "MIT",
  "author": "kityandhero <kityandhero@126.com>",
  "main": "src/index.ts",
  "scripts": {
    "prepare": "husky install",
    "ncuAll": "ncu -u",
    "scp": "git pull && git stage -A && git commit --message \"fix: auto-update\" && git push && git push",
    "cz": "cz",
    "scz": "git pull && git stage -A && npm run cz && git push",
    "commitlint": "npx commitlint --edit",
    "clean": "rimraf ./src/.umi* && rimraf ./node_modules && rimraf ./yarn-error.log && rimraf ./yarn.lock && rimraf ./package-lock.json",
    "clean:framework": "rimraf ./src/.umi* && rimraf ./node_modules/antd-management-fast-framework",
    "reinstall": "npm run clean && git pull && antd-management-fast-cli env --agent http://127.0.0.1:7890 && pnpm install",
    "reinstall:start": "npm run reinstall && npm start",
    "postinstall": "umi g tmp",
    "analyze-dev": "cross-env ANALYZE=1 umi dev",
    "analyze-build": "cross-env ANALYZE=1 umi build",
    "build": "cross-env UMI_ENV=production umi build",
    "deploy": "npm run site && npm run gh-pages",
    "docker-hub:build": "docker build  -f Dockerfile.hub -t  ant-design-pro ./",
    "docker-prod:build": "docker-compose -f ./docker/docker-compose.yml build",
    "docker-prod:dev": "docker-compose -f ./docker/docker-compose.yml up",
    "docker:build": "docker-compose -f ./docker/docker-compose.dev.yml build",
    "docker:dev": "docker-compose -f ./docker/docker-compose.dev.yml up",
    "docker:push": "npm run docker-hub:build && npm run docker:tag && docker push antdesign/ant-design-pro",
    "docker:tag": "docker tag ant-design-pro antdesign/ant-design-pro",
    "gh-pages": "cp CNAME ./dist/ && gh-pages -d dist",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "lint": "umi g tmp && npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint:prettier": "prettier --check \"**/*\" --end-of-line auto",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:style": "stylelint --fix \"src/**/*.less\"",
    "prettier": "prettier -c --write \"**/*\"",
    "site": "npm run fetch:blocks && npm run build",
    "start": "cross-env PORT=8842 UMI_UI=none UMI_ENV=development umi dev",
    "dev": "npm run start:dev",
    "start:dev": "cross-env PORT=8842 REACT_APP_ENV=dev MOCK=none UMI_ENV=development umi dev",
    "start:dev-no-ui": "cross-env PORT=8842 REACT_APP_ENV=dev MOCK=none UMI_ENV=development UMI_UI=none umi dev",
    "start:no-mock": "cross-env PORT=8842 MOCK=none UMI_ENV=development umi dev",
    "start:ui": "cross-env PORT=8842 UMI_ENV=development umi dev",
    "start:pre": "cross-env PORT=8842 REACT_APP_ENV=pre UMI_ENV=development umi dev",
    "start:test": "cross-env PORT=8842 REACT_APP_ENV=test MOCK=none UMI_ENV=development umi dev",
    "pretest": "node ./tests/beforeTest",
    "test": "umi test",
    "test:all": "node ./tests/run-tests.js",
    "test:component": "umi test ./src/components",
    "tsc": "tsc"
  },
  "dependencies": {},
  "devDependencies": {},
  "optionalDependencies": {
    "antd-management-fast-framework": "^1.12.52",
    "antd-management-fast-cli": "^1.0.18"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "gitHead": "bff59dd6f73f7876bf07eddc292fdc8ee7048059"
}
